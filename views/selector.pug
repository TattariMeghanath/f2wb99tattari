extends layout

block content
  h1= title
  - var image_names = ["teacher.jpg", "student.webp", "school.jpg", "playing.jpg", "dancing.webp", "reading.jpg"]

  - for(let i = 0; i < 3; i++) {
    - let number = Math.floor(Math.random() * 5)
    
        img(src="/images/"+image_names[number], alt="different images", height="300px", width="300px")
  - }